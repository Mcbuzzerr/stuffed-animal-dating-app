{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Spring": {
    "Application": {
      "Name": "gateway"
    }
  },
  "Eureka": {
    "Client": {
      "ServiceUrl": "http://eureka:8761/eureka/",
      "ValidateCertificates": false,
      "ShouldRegisterWithEureka": true,
      "ShouldFetchRegistry": true
    },
    "Instance": {
      "NonSecurePort": 80,
      "HostName": "gateway",
      "InstanceId": "gateway:80"
    }
  },
  "AllowedHosts": "*"
}